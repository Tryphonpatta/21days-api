model Goal {
    id        String   @id @default(cuid())
    name      String
    note      String
    streak    Int      @default(0)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    userId    String
    color     String
    tagId     String?
    tag       Tag?     @relation(fields: [tagId], references: [id])
    user      User     @relation(fields: [userId], references: [id])
}
